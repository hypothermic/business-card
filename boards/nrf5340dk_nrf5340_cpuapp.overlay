/ {
	aliases {
		red-led = &led0;
		green-led = &led1;
		blue-led = &led3;
	};

	chosen {
		zephyr,console = &cdc_acm_uart0;
	};

	ramdisk0 {
		compatible = "zephyr,ram-disk";
		disk-name = "RAM";
		sector-size = <512>;
		sector-count = <192>;
	};
};

&zephyr_udc0 {
	cdc_acm_uart0: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};
